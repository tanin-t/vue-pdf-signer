{
  "name": "vue-pdf-signer",
  "author": "tanin-t",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/vue-pdf-signer.cjs.js",
  "module": "./dist/vue-pdf-signer.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/vue-pdf-signer.es.js",
      "require": "./dist/vue-pdf-signer.cjs.js"
    },
    "./dist/vue-pdf-signer.css": "./dist/vue-pdf-signer.css",
    "./style.css": "./dist/vue-pdf-signer.css"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.vue"
  ],
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "BUILD_LIB=true vite build",
    "build-lib": "BUILD_LIB=true vite build",
    "build-app": "vite build",
    "preview": "vite preview",
    "test:unit": "vue-cli-service test:unit",
    "publish-npm": "bash publish-npm.sh",
    "lint": "vue-cli-service lint",
    "dev:legacy": "vue-cli-service serve",
    "build-lib:legacy": "vue-cli-service build --target lib --name vue-pdf-signer src/lib.ts"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "dependencies": {
    "@mdi/js": "^6.7.96",
    "fabric": "tanin-t/fabric.js#v5-2-4",
    "lodash": "^4.17.21",
    "pdf-lib": "^1.17.1",
    "vue-pdf": "^4.3.0"
  },
  "devDependencies": {
    "vue": "^2.6.14",
    "@types/fabric": "^4.5.7",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "open": "^8.4.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "uglify-js": "^3.17.0",
    "vite": "^4.5.5",
    "vite-plugin-vue2": "^2.0.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^1.8.27"
  }
}
